    "commitlint": "17.0.3",
    "concurrently": "7.3.0",
    "cross-env": "7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-typescript": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^4.3.8",
    "json-server": "0.17.0",
