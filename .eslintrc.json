{
  "parser": "@babel/eslint-parser",
  "extends": ["prettier"],
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "@next/next/google-font-preconnect": "off",
    "semi": ["error", "never"],
    "no-restricted-imports": [
      "warn",
      {
        "patterns": ["../../../"]
      }
    ]
  },
  "plugins": ["prettier", "testing-library"]
}
